{
    "compilerOptions": {
        "baseUrl": "src",
        "target": "es6",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "outDir": "./build",
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "noImplicitAny": false,
        "strict": false
        //"strict": true,
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src", "src/assets"],
    "exclude": ["node_modules"]
}
